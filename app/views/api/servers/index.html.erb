<p id="notice"><%= notice %></p>

<h1>Api Servers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_servers.each do |api_server| %>
      <tr>
        <td><%= link_to 'Show', api_server %></td>
        <td><%= link_to 'Edit', edit_api_server_path(api_server) %></td>
        <td><%= link_to 'Destroy', api_server, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Server', new_api_server_path %>
